import numpy as np

class BackPressureEngine:
    def __init__(self, d_eff=0.656):
        self.d_eff = d_eff
        self.anti_matter_threshold = 0.95 # Threshold for vaporization/snap

    def calculate_vented_stability(self, z_number, geometry="tetrahedral"):
        """
        Evaluates stability based on NF Back-Pressure displacement.
        """
        # Displacement is proportional to Z
        raw_displacement = z_number * (1 - self.d_eff)
        
        # Venting efficiency: Simplex is high, Spherical is zero
        venting_coeff = 0.88 if geometry == "tetrahedral" else 0.05
        
        # Back-pressure is displacement minus what we can vent
        back_pressure = raw_displacement * (1 - venting_coeff)
        
        # Return Integrity (Inverse of Back-pressure)
        integrity = 1 / (1 + back_pressure)
        return integrity

# RUNNING THE REFINED TEST
